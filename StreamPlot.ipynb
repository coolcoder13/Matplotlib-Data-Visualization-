   "source": [
    "#Stream Plot\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x=y=np.arange(0,10)\n",
    "X,Y = np.meshgrid(x,y)\n",
    "u=np.ones((10,10))         #x_component to the right\n",
    "v=np.zeros((10,10))        #y_component zero\n",
    "\n",
    "plt.figure(figsize=(10,5))\n",
    "plt.streamplot(X,Y,u,v,density=0.5)\n",
    "plt.show()"
   ]
